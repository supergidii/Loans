         step=\"100\"\r\n          required\r\n          class=\"form-control\"\r\n        />\r\n        <small class=\"text-muted\">Minimum investment: $100</small>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label>Investment Duration</label>\r\n        <div class=\"duration-options\">\r\n          <button\r\n            v-for=\"days in durationOptions\"\r\n            :key=\"days\"\r\n            type=\"button\"\r\n            :class=\"['duration-option', { active: formData.duration === days }]\"\r\n            @click=\"formData.duration = days\"\r\n          >\r\n            {{ days }} Days\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"investment-summary\" v-if=\"formData.amount && formData.duration\">\r\n        <h3>Investment Summary</h3>\r\n        <div class=\"summary-item\">\r\n          <span>Principal Amount:</span>\r\n          <span>${{ formData.amount.toFixed(2) }}</span>\r\n        </div>\r\n        <div class=\"summary-item\">\r\n          <span>Duration:</span>\r\n          <span>{{ formData.duration }} Days</span>\r\n        </div>\r\n        <div class=\"summary-item\">\r\n          <span>Interest Rate:</span>\r\n          <span>{{ interestRate }}%</span>\r\n        </div>\r\n        <div class=\"summary-item highlight\">\r\n          <span>Expected Return:</span>\r\n          <span>${{ expectedReturn.toFixed(2) }}</span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <button type=\"submit\" class=\"btn btn-primary\" :disabled=\"loading\">\r\n          {{ loading ? 'Creating Investment...' : 'Create Investment' }}\r\n        </button>\r\n      </div>\r\n\r\n      <div v-if=\"error\" class=\"alert alert-danger\">\r\n        {{ error }}\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from 'vue';\r\nimport api from '../api';\r\n\r\nexport default {\r\n  name: 'InvestmentForm',\r\n  emits: ['investment-created'],\r\n  setup(props, { emit }) {\r\n    const formData = ref({\r\n      amount: 100,\r\n      duration: 4\r\n    });\r\n\r\n    const loading = ref(false);\r\n    const error = ref('');\r\n    const interestRate = 10; // 10% annual interest rate\r\n\r\n    const durationOptions = [4, 8, 16, 30];\r\n\r\n    const expectedReturn = computed(() => {\r\n      const principal = formData.value.amount;\r\n      const rate = interestRate / 100;\r\n      const time = formData.value.duration / 365; // Convert days to years\r\n      return principal * (1 + (rate * time));\r\n    });\r\n\r\n    const createInvestment = async () => {\r\n      try {\r\n        loading.value = true;\r\n        error.value = '';\r\n\r\n        const response = await api.post('/investments', formData.value);\r\n        \r\n        if (response.data.success) {\r\n          // Reset form\r\n          formData.value = {\r\n            amount: 100,\r\n            duration: 4\r\n          };\r\n          \r\n          // Emit success event\r\n          emit('investment-created', response.data.data);\r\n        }\r\n      } catch (err) {\r\n        error.value = err.response?.data?.message || 'Failed to create investment';\r\n      } finally {\r\n        loading.value = false;\r\n      }\r\n    };\r\n\r\n    return {\r\n      formData,\r\n      loading,\r\n      error,\r\n      interestRate,\r\n      durationOptions,\r\n      expectedReturn,\r\n      createInvestment\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.investment-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-control {\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\n.duration-options {\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 1fr);\r\n  gap: 10px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.duration-option {\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  background: white;\r\n  cursor: pointer;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.duration-option:hover {\r\n  border-color: #007bff;\r\n}\r\n\r\n.duration-option.active {\r\n  background: #007bff;\r\n  color: white;\r\n  border-color: #007bff;\r\n}\r\n\r\n.investment-summary {\r\n  background: #f8f9fa;\r\n  padding: 20px;\r\n  border-radius: 4px;\r\n  margin: 20px 0;\r\n}\r\n\r\n.summary-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin: 10px 0;\r\n}\r\n\r\n.summary-item.highlight {\r\n  font-weight: bold;\r\n  color: #28a745;\r\n  font-size: 1.1em;\r\n  margin-top: 15px;\r\n  padding-top: 15px;\r\n  border-top: 1px solid #ddd;\r\n}\r\n\r\n.btn-primary {\r\n  width: 100%;\r\n  padding: 12px;\r\n  background: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background: #0056b3;\r\n}\r\n\r\n.btn-primary:disabled {\r\n  background: #ccc;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.alert-danger {\r\n  background: #f8d7da;\r\n  color: #721c24;\r\n  padding: 10px;\r\n  border-radius: 4px;\r\n  margin-top: 10px;\r\n}\r\n</style> "],"mappings":";;AAmEA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,OAAOC,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,KAAK,EAAE,CAAC,oBAAoB,CAAC;EAC7BC,KAAK,WAALA,KAAKA,CAACC,KAAK,EAAAC,IAAA,EAAY;IAAA,IAARC,IAAG,GAAAD,IAAA,CAAHC,IAAG;IAChB,IAAMC,QAAO,GAAIT,GAAG,CAAC;MACnBU,MAAM,EAAE,GAAG;MACXC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,IAAMC,OAAM,GAAIZ,GAAG,CAAC,KAAK,CAAC;IAC1B,IAAMa,KAAI,GAAIb,GAAG,CAAC,EAAE,CAAC;IACrB,IAAMc,YAAW,GAAI,EAAE,EAAE;;IAEzB,IAAMC,eAAc,GAAI,CAAC,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,EAAE,CAAC;IAEtC,IAAMC,cAAa,GAAIf,QAAQ,CAAC,YAAM;MACpC,IAAMgB,SAAQ,GAAIR,QAAQ,CAACS,KAAK,CAACR,MAAM;MACvC,IAAMS,IAAG,GAAIL,YAAW,GAAI,GAAG;MAC/B,IAAMM,IAAG,GAAIX,QAAQ,CAACS,KAAK,CAACP,QAAO,GAAI,GAAG,EAAE;MAC5C,OAAOM,SAAQ,IAAK,IAAKE,IAAG,GAAIC,IAAK,CAAC;IACxC,CAAC,CAAC;IAEF,IAAMC,gBAAe;MAAA,IAAAC