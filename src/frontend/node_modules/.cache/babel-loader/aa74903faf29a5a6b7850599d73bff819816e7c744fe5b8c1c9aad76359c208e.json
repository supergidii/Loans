template>\r\n  <div class=\"investment-form\">\r\n    <h2>Create Investment</h2>\r\n    <form @submit.prevent=\"createInvestment\">\r\n      <div class=\"form-group\">\r\n        <label for=\"amount\">Investment Amount ($)</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"amount\"\r\n          v-model=\"formData.amount\"\r\n          min=\"100\"\r\n          step=\"1\"\r\n          required\r\n        />\r\n      </div>\r\n      \r\n      <div class=\"form-group\">\r\n        <label for=\"duration\">Duration</label>\r\n        <select\r\n          id=\"duration\"\r\n          v-model=\"formData.duration\"\r\n          required\r\n        >\r\n          <option v-for=\"option in durationOptions\" \r\n                  :key=\"option.value\" \r\n                  :value=\"option.value\">\r\n            {{ option.label }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"summary\" v-if=\"expectedReturn\">\r\n        <h3>Investment Summary</h3>\r\n        <p>Amount: ${{ formData.amount }}</p>\r\n        <p>Duration: {{ formData.duration }} days</p>\r\n        <p>Expected Return: ${{ expectedReturn.toFixed(2) }}</p>\r\n      </div>\r\n\r\n      <div v-if=\"error\" class=\"error-message\">\r\n        {{ error }}\r\n      </div>\r\n\r\n      <button type=\"submit\" :disabled=\"loading\">\r\n        {{ loading ? 'Creating...' : 'Create Investment' }}\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'InvestmentForm',\r\n  emits: ['investment-created'],\r\n  setup(props, { emit }) {\r\n    const formData = ref({\r\n      amount: 100,\r\n      duration: 30\r\n    });\r\n\r\n    const loading = ref(false);\r\n    const error = ref('');\r\n    const interestRate = 10; // 10% annual interest rate\r\n\r\n    const durationOptions = [\r\n      { value: 4, label: '4 days' },\r\n      { value: 8, label: '8 days' },\r\n      { value: 16, label: '16 days' },\r\n      { value: 30, label: '30 days' }\r\n    ];\r\n\r\n    const expectedReturn = computed(() => {\r\n      const principal = formData.value.amount;\r\n      const rate = interestRate / 100;\r\n      const time = formData.value.duration / 365; // Convert days to years\r\n      return principal * (1 + (rate * time));\r\n    });\r\n\r\n    const createInvestment = async () => {\r\n      try {\r\n        loading.value = true;\r\n        error.value = '';\r\n\r\n        console.log('Creating investment with data:', formData.value);\r\n        const response = await axios.post('/api/investments', formData.value);\r\n        \r\n        if (response.data.success) {\r\n          // Reset form\r\n          formData.value = {\r\n            amount: 100,\r\n            duration: 30\r\n          };\r\n          \r\n          // Emit success event\r\n          emit('investment-created', response.data.data);\r\n        }\r\n      } catch (err) {\r\n        console.error('Investment creation error:', err);\r\n        error.value = err.response?.data?.message || 'Failed to create investment';\r\n      } finally {\r\n        loading.value = false;\r\n      }\r\n    };\r\n\r\n    return {\r\n      formData,\r\n      loading,\r\n      error,\r\n      interestRate,\r\n      durationOptions,\r\n      expectedReturn,\r\n      createInvestment\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.investment-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-control {\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\n.duration-options {\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 1fr);\r\n  gap: 10px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.duration-option {\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  background: white;\r\n  cursor: pointer;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.duration-option:hover {\r\n  border-color: #007bff;\r\n}\r\n\r\n.duration-option.active {\r\n  background: #007bff;\r\n  color: white;\r\n  border-color: #007bff;\r\n}\r\n\r\n.investment-summary {\r\n  background: #f8f9fa;\r\n  padding: 20px;\r\n  border-radius: 4px;\r\n  margin: 20px 0;\r\n}\r\n\r\n.summary-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin: 10px 0;\r\n}\r\n\r\n.summary-item.highlight {\r\n  font-weight: bold;\r\n  color: #28a745;\r\n  font-size: 1.1em;\r\n  margin-top: 15px;\r\n  padding-top: 15px;\r\n  border-top: 1px solid #ddd;\r\n}\r\n\r\n.btn-primary {\r\n  width: 100%;\r\n  padding: 12px;\r\n  background: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background: #0069d9;\r\n}\r\n\r\n.btn-primary:disabled {\r\n  background: #6c757d;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.alert-danger {\r\n  color: #721c24;\r\n  background-color: #f8d7da;\r\n  border: 1px solid #f5c6cb;\r\n  padding: 10px;\r\n  border-radius: 4px;\r\n  margin-top: 10px;\r\n}\r\n</style> "],"mappings":";;AAkDA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,KAAK,EAAE,CAAC,oBAAoB,CAAC;EAC7BC,KAAK,WAALA,KAAKA,CAACC,KAAK,EAAAC,IAAA,EAAY;IAAA,IAARC,IAAG,GAAAD,IAAA,CAAHC,IAAG;IAChB,IAAMC,QAAO,GAAIT,GAAG,CAAC;MACnBU,MAAM,EAAE,GAAG;MACXC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,IAAMC,OAAM,GAAIZ,GAAG,CAAC,KAAK,CAAC;IAC1B,IAAMa,KAAI,GAAIb,GAAG,CAAC,EAAE,CAAC;IACrB,IAAMc,YAAW,GAAI,EAAE,EAAE;;IAEzB,IAAMC,eAAc,GAAI,CACtB;MAAEC,KAAK,EAAE,CAAC;MAAEC,KAAK,EAAE;IAAS,CAAC,EAC7B;MAAED,KAAK,EAAE,CAAC;MAAEC,KAAK,EAAE;IAAS,CAAC,EAC7B;MAAED,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAU,CAAC,EAC/B;MAAED,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAU,EAC/B;IAED,IAAMC,cAAa,GAAIjB,QAAQ,CAAC,YAAM;MACpC,IAAMkB,SAAQ,GAAIV,QAAQ,CAACO,KAAK,CAACN,MAAM;MACvC,IAAMU,IAAG,GAAIN,YAAW,GAAI,GAAG;MAC/B,IAAMO,IAAG